<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping>
    <class name="com.custom" table="bs_user_auth">
        <id name="id" type="java.lang.String">
            <column name="id" length="40" />
            <generator class="assigned" />
        </id>
        <property name="userId" type="java.lang.String">
            <column name="user_id" length="20"/>
        </property>
        <property name="authId" type="java.lang.String">
            <column name="auth_id" length="40" />
        </property>
           <property name="createDate" type="java.util.Date">
            <column name="create_date" length="40" />
        </property>
        <property name="modifyDate" type="java.util.Date">
            <column name="modify_date" length="40" />
        </property>
        <property name="createUser" type="java.lang.String">
            <column name="create_user" length="40" />
        </property>
        <property name="modifyUser" type="java.lang.String">
            <column name="modify_user" length="40" />
        </property>
    </class>
</hibernate-mapping>
