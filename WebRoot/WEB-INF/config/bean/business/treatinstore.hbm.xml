        <property name="name" type="java.lang.String">
            <column name="name" length="50"/>
        </property>
        <property name="unity" type="java.lang.String">
            <column name="unity" length="50"/>
        </property>
        <property name="innum" type="java.lang.Integer">
            <column name="innum" length="50"/>
