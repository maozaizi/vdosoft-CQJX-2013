<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping>
    <class name="com.osWorkflowdefs" table="OS_WORKFLOWDEFS">
    	<id name="wfName" type="java.lang.String">
            <column name="wf_name" length="100" />
            <generator class="assigned" />
